{{#if description}}
/**
 * {{{description}}}
 */
{{/if}}
export interface {{{name}}}{{>extends}} {
    {{#each properties}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    {{>isReadOnly}}{{{name}}}{{>isRequired}}: {{#if @root.useDateType}}{{#equals format 'date-time'}}Date{{else equals format 'date'}}Date{{else}}{{>type parent=../name}}{{/equals}}{{~else~}}{{>type parent=../name}}{{/if}};
    {{/each}}
}
{{#if enums}}
{{#unless @root.useUnionTypes}}

export namespace {{{name}}} {

    {{#each enums}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    export enum {{{name}}} {
        {{#each enum}}
        {{{name}}} = {{{value}}},
        {{/each}}
    }

    {{/each}}

}
{{/unless}}
{{/if}}
